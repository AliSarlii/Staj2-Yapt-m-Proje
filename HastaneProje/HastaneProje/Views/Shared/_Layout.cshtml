<!DOCTYPE html>
<html class="no-js" lang="tr">
    <head>
        <link rel="icon" href="~/Content/HomeController/img/Favicon.ico">

        <link href="https://fonts.googleapis.com/css?family=Poppins:200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">
        <link rel="stylesheet" href="~/Content/HomeController/css/bootstrap.min.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/nice-select.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/font-awesome.min.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/icofont.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/slicknav.min.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/owl-carousel.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/datepicker.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/animate.min.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/magnific-popup.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/normalize.css">
        <link rel="stylesheet" href="~/Content/HomeController/style.css">
        <link rel="stylesheet" href="~/Content/HomeController/css/responsive.css">

        <title>@ViewBag.Title</title>

        <style>
            .header .nav li.active a {
                color: #4724da !important;
            }
            .header .nav li a::before {
                background-color: #4724da !important;
            }
            .slider .owl-carousel .owl-nav div {
                background-color: #4724da !important;
            }
            a:hover {
            color: #4724da !important;
            }
            .btn {
                background-color: #4724da !important;
            }
            .Feautes .single-features .signle-icon i:hover {
                background-color: #4724da !important;
            }
            .Feautes .single-features .signle-icon i:hover::before {
                color: white !important;
            }
            .Feautes .single-features h3:hover {
                background-color:white;
            }
            .schedule .single-schedule .inner {
                background-color: #4724da !important;
            }
            .preloader::before, .preloader::after {
                background-color: #4724da !important;
            }
            .footer .footer-top {
                background-color: #4724da !important;
            }
            ul.nav.menu{
                justify-content:flex-end
            }
            input[type=number]::-webkit-inner-spin-button,
            input[type=number]::-webkit-outer-spin-button {
                -webkit-appearance: none;
                margin: 0;
            }
        </style>
    </head>
    <body>
        <div class="preloader">
            <div class="loader">
                <div class="loader-outter"></div>
                <div class="loader-inner"></div>

                <div class="indicator">
                    <svg width="16px" height="12px">
                        <polyline id="back" points="1 6 4 6 6 11 10 1 12 6 15 6"></polyline>
                        <polyline id="front" points="1 6 4 6 6 11 10 1 12 6 15 6"></polyline>
                    </svg>
                </div>
            </div>
        </div>

        <header class="header">
            <div class="header-inner">
                <div class="container">
                    <div class="inner">
                        <div class="row">
                            <div class="col-lg-3 col-md-3 col-12">
                                <div class="logo">
                                    <a href="/Home/Home/"><img src="~/Content/HomeController/img/Logo.png" alt="#"></a>
                                </div>
                                <div class="mobile-nav"></div>
                            </div>
                            <div class="col-lg-7 col-md-9 col-12">
                                <div class="main-menu">
                                    <nav class="navigation">
                                        <ul class="nav menu">
                                            <li><a href="/Home/Makine_Ogrenmesi/">Makine Öğrenmesi</a></li>
                                            <li><a href="/Home/Hakkinda/">Hakkında</a></li>
                                            <li><a href="/Home/Iletisim/">İletişim</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                            <div class="col-lg-2 col-12">
                                <div class="get-quote">
                                    <a href="/Home/Giris/" class="btn">Giriş</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        @RenderBody()

        <footer id="footer" class="footer">
            <div class="footer-top">
                <div class="container">
                    <div class="row">
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-footer">
                                <h2>Hakkımızda</h2>
                                <p>Lorem ipsum dolor sit am consectetur adipisicing elit do eiusmod tempor incididunt ut labore dolore magna.</p>
                                <ul class="social">
                                    <li><a href="#"><i class="icofont-facebook"></i></a></li>
                                    <li><a href="#"><i class="icofont-google-plus"></i></a></li>
                                    <li><a href="#"><i class="icofont-twitter"></i></a></li>
                                    <li><a href="#"><i class="icofont-vimeo"></i></a></li>
                                    <li><a href="#"><i class="icofont-pinterest"></i></a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-footer f-link">
                                <h2>Hızlı Bağlantılar</h2>
                                <div class="row">
                                    <div class="col-lg-6 col-md-6 col-12">
                                        <ul>
                                            <li><a href="/Home/Home/"><i class="fa fa-caret-right" aria-hidden="true"></i>Ana Sayfa</a></li>
                                            <li><a href="/Home/Hakkinda/"><i class="fa fa-caret-right" aria-hidden="true"></i>Hakkımızda</a></li>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Hizmetler</a></li>
                                            <li><a href="/Home/Makine_Ogrenmesi/"><i class="fa fa-caret-right" aria-hidden="true"></i>Makine Öğrenmesi</a></li>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Diğer Bağlantılar</a></li>
                                        </ul>
                                    </div>
                                    <div class="col-lg-6 col-md-6 col-12">
                                        <ul>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Danışmanlık</a></li>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Finans</a></li>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>Görüşler</a></li>
                                            <li><a href="#"><i class="fa fa-caret-right" aria-hidden="true"></i>SSS</a></li>
                                            <li><a href="/Home/Iletisim/"><i class="fa fa-caret-right" aria-hidden="true"></i>İletişim</a></li>
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-footer">
                                <h2>Açık Saatler</h2>
                                <p>Lorem ipsum dolor sit ame consectetur adipisicing elit do eiusmod tempor incididunt.</p>
                                <ul class="time-sidual">
                                    <li class="day">Pazartesi - Cuma <span>08.00-20.00</span></li>
                                    <li class="day">Cumartesi <span>09.00-18.30</span></li>
                                    <li class="day">Pazartesi - Perşembe <span>09.00-15.00</span></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-lg-3 col-md-6 col-12">
                            <div class="single-footer">
                                <h2>Bülten</h2>
                                <p>Bültenimize abone olarak tüm haberlerimizi e-posta kutunuza alın. Lorem ipsum dolor sit amet, consectetur adipisicing elit,</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-bottom bg-white">
                <div class="container">
                    <div class="row align-items-center">

                        <div class="copyright m-auto" style="background-color: white; color: #4724da">
                            <p>&copy; 2024 Hastane Otomasyonu Tüm Hakları Saklıdır.</p>
                        </div>

                    </div>
                </div>
            </div>

            <div >
                <p class="text-right" style="margin:0px 10px 10px 0px; background-color:white; box-sizing:border-box;">Made by <i><a style="text-decoration:none; color:gray;" href="https://www.linkedin.com/in/ali-şarlı-08044923b" target="_blank">Ali Şarlı</a></i></p>
            </div>

        </footer>
        <script src="~/Content/HomeController/js/jquery.min.js"></script>
        <script src="~/Content/HomeController/js/jquery-migrate-3.0.0.js"></script>
        <script src="~/Content/HomeController/js/jquery-ui.min.js"></script>
        <script src="~/Content/HomeController/js/popper.min.js"></script>
        <script src="~/Content/HomeController/js/bootstrap.min.js"></script>
        <script src="~/Content/HomeController/js/colors.js"></script>
        <script src="~/Content/HomeController/js/slicknav.min.js"></script>
        <script src="~/Content/HomeController/js/owl-carousel.js"></script>
        <script src="~/Content/HomeController/js/main.js"></script>
        <script src="https://kit.fontawesome.com/2765b9c277.js" crossorigin="anonymous"></script>

        @RenderSection("scripts", required: false)
    </body>
</html>